# Zad 1

def print_unique_values(list):
    unique_values = []
    for value in list:
        if value not in unique_values:
            unique_values.append(value)
            print(value)

list = [7,8,1,2,3,4,9]
print_unique_values(list)


word = "Lorem ipsum dolor sit amet, consectetur adipiscing elit"
reversed_word = word[::-1].replace(" ", "").replace(",", "").replace(".", "")
fourth_letters = reversed_word[3::4]
print(fourth_letters)
